{
  "name": "FreePromptHub - Free AI Prompts",
  "short_name": "FreePromptHub",
  "description": "Free AI prompts for ChatGPT, Claude, and more. No signup required.",
  "start_url": "/",
  "id": "freeprompthub-app",
  "scope": "/",
  "display": "standalone",
  "display_override": ["window-controls-overlay", "standalone", "minimal-ui"],
  "background_color": "#ffffff",
  "theme_color": "#0066CC",
  "orientation": "portrait-primary",
  "edge_side_panel": {
    "preferred_width": 400
  },
  "launch_handler": {
    "client_mode": "navigate-existing"
  },
  "icons": [
    {
      "src": "/favicon.svg",
      "sizes": "any",
      "type": "image/svg+xml",
      "purpose": "any maskable"
    },
    {
      "src": "/icon-192x192.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "any maskable"
    },
    {
      "src": "/icon-512x512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any maskable"
    }
  ],
  "shortcuts": [
    {
      "name": "Search Prompts",
      "short_name": "Search",
      "description": "Search through 150+ AI prompts",
      "url": "/search",
      "icons": [
        {
          "src": "/icon-192x192.png",
          "sizes": "192x192",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Business Prompts",
      "short_name": "Business",
      "description": "Professional business prompts",
      "url": "/prompts/business/",
      "icons": [
        {
          "src": "/icon-192x192.png",
          "sizes": "192x192",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Coding Prompts",
      "short_name": "Coding",
      "description": "Development and programming prompts",
      "url": "/prompts/coding/",
      "icons": [
        {
          "src": "/icon-192x192.png",
          "sizes": "192x192",
          "type": "image/png"
        }
      ]
    }
  ],
  "file_handlers": [
    {
      "action": "/share",
      "accept": {
        "text/plain": [".txt"],
        "text/markdown": [".md"]
      }
    }
  ],
  "protocol_handlers": [
    {
      "protocol": "web+freeprompthub",
      "url": "/share?url=%s"
    }
  ],
  "categories": ["productivity", "utilities", "education", "business"],
  "lang": "en-US",
  "dir": "ltr",
  "prefer_related_applications": false,
  "related_applications": [],
  "screenshots": [
    {
      "src": "/screenshot-wide.png",
